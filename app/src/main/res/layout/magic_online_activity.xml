<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qianfeng.housefinish.ui.DayDayAwardActivity">
    <android.support.design.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/main_height"
        android:elevation="@dimen/elevation_height"
        android:background="@color/touming">

        <Button
            android:id="@+id/magic_online_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:drawableLeft="@mipmap/icon_back_gary"
            android:background="@color/touming"
            android:layout_marginLeft="10dp"/>
    </android.support.design.widget.CoordinatorLayout>

<WebView

    android:id="@+id/magic_online_webview"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

</WebView>
</FrameLayout>
